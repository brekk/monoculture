{
  "name": "monoculture",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*",
    "shared/*",
    "tools/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@swc/core": "1.3.31",
    "@swc/jest": "0.2.24",
    "@swc/register": "0.1.10",
    "@types/jest": "29.2.5",
    "@types/testing-library__jest-dom": "5.14.5",
    "dotenv-cli": "6.0.0",
    "execa": "6.1.0",
    "husky": "8.0.0",
    "jest": "^29.4.2",
    "plop": "^4.0.0",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18.15.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19",
  "peerDependencies": {},
  "scripts": {
    "nps": "dotenv -- nps -c ./package-scripts.cjs",
    "build": "dotenv -- nps -c ./package-scripts.cjs build",
    "copy": "dotenv -- nps -c ./package-scripts.cjs copy",
    "copy:rebuild": "dotenv -- nps -c ./package-scripts.cjs copy.rebuild",
    "deploy": "dotenv -- nps -c ./package-scripts.cjs deploy",
    "dev": "dotenv -- nps -c ./package-scripts.cjs dev",
    "lint": "dotenv -- nps -c ./package-scripts.cjs lint",
    "meta": "dotenv -- nps -c ./package-scripts.cjs meta",
    "meta:docs": "dotenv -- nps -c ./package-scripts.cjs meta.docs",
    "meta:graph": "dotenv -- nps -c ./package-scripts.cjs meta.graph",
    "prepare": "dotenv -- nps -c ./package-scripts.cjs prepare",
    "proxy": "dotenv -- nps -c ./package-scripts.cjs proxy",
    "readme": "dotenv -- nps -c ./package-scripts.cjs readme",
    "syncScripts": "dotenv -- nps -c ./package-scripts.cjs syncScripts",
    "test": "dotenv -- nps -c ./package-scripts.cjs test",
    "test:all": "dotenv -- nps -c ./package-scripts.cjs test.all",
    "test:watch": "dotenv -- nps -c ./package-scripts.cjs test.watch"
  }
}
