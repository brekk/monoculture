    "apps/*",
    "packages/*",
    "shared/*",
    "tools/*"
  ],
  "devDependencies": {
    "@action-validator/cli": "0.5.4",
    "@action-validator/core": "0.5.4",
    "@babel/core": "7.23.6",
    "@swc/core": "1.3.101",
    "@swc/jest": "0.2.29",
    "@swc/register": "0.1.10",
    "@types/jest": "29.5.11",
    "@types/node": "^20.10.5",
    "dotenv-cli": "7.3.0",
    "esbuild": "0.19.10",
    "eslint": "8.56.0",
    "execa": "8.0.1",
    "husky": "8.0.3",
    "istanbul-lib-coverage": "^3.2.2",
    "istanbul-lib-report": "^3.0.1",
    "istanbul-merge": "^2.0.0",
    "istanbul-reports": "^3.1.6",
    "jest": "29.7.0",
    "knip": "^3.8.4",
    "lint-staged": "15.2.0",
    "nps": "5.10.0",
    "nps-utils": "1.7.0",
    "nyc": "15.1.0",
    "plop": "4.0.0",
    "prettier": "3.1.1",
    "ramda": "0.29.1",
    "turbo": "1.11.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.15.0"
  },
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "nps": "dotenv -- nps -c ./package-scripts.cjs",
    "build": "dotenv -- nps -c ./package-scripts.cjs build",
    "care": "dotenv -- nps -c ./package-scripts.cjs care",
    "deploy": "dotenv -- nps -c ./package-scripts.cjs deploy",
    "lint": "dotenv -- nps -c ./package-scripts.cjs lint",
    "meta": "dotenv -- nps -c ./package-scripts.cjs meta",
    "meta:docs": "dotenv -- nps -c ./package-scripts.cjs meta.docs",
    "meta:graph": "dotenv -- nps -c ./package-scripts.cjs meta.graph",
    "meta:readme": "dotenv -- nps -c ./package-scripts.cjs meta.readme",
    "prepare": "dotenv -- nps -c ./package-scripts.cjs prepare",
    "readme": "dotenv -- nps -c ./package-scripts.cjs readme",
    "sync": "dotenv -- nps -c ./package-scripts.cjs sync",
    "sync:scripts": "dotenv -- nps -c ./package-scripts.cjs sync.scripts",
    "test": "dotenv -- nps -c ./package-scripts.cjs test",
    "test:ci": "dotenv -- nps -c ./package-scripts.cjs test.ci",
    "test:ciCopy": "dotenv -- nps -c ./package-scripts.cjs test.ciCopy",
    "test:snapshot": "dotenv -- nps -c ./package-scripts.cjs test.snapshot",
    "test:watch": "dotenv -- nps -c ./package-scripts.cjs test.watch",
    "workflow": "dotenv -- nps -c ./package-scripts.cjs workflow",
    "workflow:validate": "dotenv -- nps -c ./package-scripts.cjs workflow.validate"
  }
