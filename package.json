{
  "name": "monoculture",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "shared/*",
    "tools/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@swc/core": "^1.3.95",
    "@swc/jest": "^0.2.29",
    "@swc/register": "^0.1.10",
    "@types/jest": "^29.5.7",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.53.0",
    "execa": "^8.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "plop": "^4.0.0",
    "prettier": "^3.0.3",
    "turbo": "^1.10.16"
  },
  "engines": {
    "node": ">=18.15.0"
  },
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "nps": "dotenv -- nps -c ./package-scripts.cjs",
    "build": "dotenv -- nps -c ./package-scripts.cjs build",
    "care": "dotenv -- nps -c ./package-scripts.cjs care",
    "deploy": "dotenv -- nps -c ./package-scripts.cjs deploy",
    "lint": "dotenv -- nps -c ./package-scripts.cjs lint",
    "meta": "dotenv -- nps -c ./package-scripts.cjs meta",
    "meta:docs": "dotenv -- nps -c ./package-scripts.cjs meta.docs",
    "meta:graph": "dotenv -- nps -c ./package-scripts.cjs meta.graph",
    "meta:readme": "dotenv -- nps -c ./package-scripts.cjs meta.readme",
    "prepare": "dotenv -- nps -c ./package-scripts.cjs prepare",
    "readme": "dotenv -- nps -c ./package-scripts.cjs readme",
    "sync": "dotenv -- nps -c ./package-scripts.cjs sync",
    "sync:scripts": "dotenv -- nps -c ./package-scripts.cjs sync.scripts",
    "test": "dotenv -- nps -c ./package-scripts.cjs test",
    "test:watch": "dotenv -- nps -c ./package-scripts.cjs test.watch"
  }
}
