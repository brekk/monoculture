    "clean:build": "dotenv -- nps -c ./package-scripts.cjs clean.build",
    "clean:next": "dotenv -- nps -c ./package-scripts.cjs clean.next",
    "clean:out": "dotenv -- nps -c ./package-scripts.cjs clean.out",
    "clean:pages": "dotenv -- nps -c ./package-scripts.cjs clean.pages",
    "dev": "dotenv -- nps -c ./package-scripts.cjs dev",
    "lint": "dotenv -- nps -c ./package-scripts.cjs lint",
    "start": "dotenv -- nps -c ./package-scripts.cjs start"
