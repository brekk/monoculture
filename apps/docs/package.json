  "scripts": {
    "nps": "dotenv -- nps -c ./package-scripts.cjs",
    "autodoc": "dotenv -- nps -c ./package-scripts.cjs autodoc",
    "autodoc:regen": "dotenv -- nps -c ./package-scripts.cjs autodoc.regen",
